<?xml version="1.0"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <artifactId>citations-base</artifactId>
    <groupId>org.sakaiproject</groupId>
    <version>M2</version>
    <relativePath>../../pom.xml</relativePath>
  </parent>
  <name>sakai-citations-pack</name>
  <groupId>org.sakaiproject</groupId>
  <artifactId>sakai-citations-pack</artifactId>
  <organization>
    <name>University of Michigan</name>
    <url>http://sakaiproject.org/</url>
  </organization>
  <inceptionYear>2003</inceptionYear>
  <packaging>sakai-component</packaging>
  <properties>
    <deploy.target>components</deploy.target>
  </properties>
  <dependencies>
    <dependency>
      <groupId>org.sakaiproject</groupId>
      <artifactId>sakai-citations-impl</artifactId>
      <version>${sakai.version}</version>
      <exclusions>
	<exclusion>
		<groupId>OKI</groupId>
		<artifactId>OkiOSID</artifactId>
	</exclusion>
	<exclusion>
		<groupId>zing</groupId>
		<artifactId>cql-java</artifactId>
	</exclusion>
	<exclusion>
		<groupId>org.sakaiproject</groupId>
		<artifactId>sakai-citations-util</artifactId>
	</exclusion>
	<exclusion>
		<groupId>org.sakaiproject</groupId>
		<artifactId>sakai-velocity-tool</artifactId>
	</exclusion>
      </exclusions>
    </dependency>
    <!-- the following are NOT dependencies of the Impl, please check they are 
	really needed -->
    <dependency>
      <groupId>org.sakaiproject</groupId>
      <artifactId>sakai-db-storage</artifactId>
      <version>${sakai.version}</version>
    </dependency>
    <dependency>
      <groupId>org.sakaiproject</groupId>
      <artifactId>sakai-entity-util</artifactId>
      <version>${sakai.version}</version>
    </dependency>
    <dependency>
      <groupId>org.sakaiproject</groupId>
      <artifactId>sakai-event-util</artifactId>
      <version>${sakai.version}</version>
    </dependency>

  </dependencies>
  <build>
    <resources/>
<!-- java source -->
    <sourceDirectory>src/java</sourceDirectory>
  </build>
</project>
